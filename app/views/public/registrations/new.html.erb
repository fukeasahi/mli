<div class="col-sm-3">
  <h2>サインアップ</h2>

  <%= form_with model: @customer, url: customer_registration_path, id: 'new_user', class: 'new_user', local: true do |f| %>
    <%= render "devise/shared/error_messages", resource: resource %>

    <div class="field">
      <%= f.label :姓 %><br />
      <%= f.text_field :last_name, autofocus: true, class:"form-control" %>
    </div>

    <div class="field">
      <%= f.label :名 %><br />
      <%= f.text_field :first_name, autofocus: true, class:"form-control" %>
    </div>

    <div class="field">
      <%= f.label :セイ %><br />
      <%= f.text_field :last_name_kana, autofocus: true, class:"form-control" %>
    </div>

    <div class="field">
      <%= f.label :メイ %><br />
      <%= f.text_field :first_name_kana, autofocus: true, class:"form-control" %>
    </div>

    <div class="field">
      <%= f.label :manabaのuserid %><br />
      <%= f.text_field :manaba_user_id, autofocus: true, class:"form-control" %>
    </div>

    <div class="field">
      <%= f.label :manabaのpassword %><br />
      <%= f.text_field :manaba_password, autofocus: true, class:"form-control" %>
    </div>

    <div class="field">
      <%= f.label :Lineのpath %><br />
      <%= f.text_field :line_path, autofocus: true, class:"form-control" %>
    </div>

    <div class="field">
      <%= f.label :メール %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email", class:"form-control" %>
    </div>

    <div class="field">
      <%= f.label :このアプリのpassword %>
      <% if @minimum_password_length %>
      <em>(<%= @minimum_password_length %> characters minimum)</em>
      <% end %><br />
      <%= f.password_field :password, autocomplete: "new-password", class:"form-control" %>
    </div>

    <div class="field">
      <%= f.label :このアプリのpasswordの確認 %><br />
      <%= f.password_field :password_confirmation, autocomplete: "new-password", class:"form-control" %>
    </div>

    <div class="actions">
      <%= f.submit "サインアップ" %>
    </div>
  <% end %>

  <%= render "public/shared/links" %>

</div>
<div class="col-sm-9">
</div>
